<launch>
	<param name="robot_description" command="$(find xacro)/xacro --inorder '$(find agv_model)/agv.xacro'" />

<group ns="robot1">
	<param name="tf_prefix" value="robot1" />
	<include file="$(find agv_model)/launch/one_robot.launch" >
	<arg name="init_pose" value="-x 0 -y -4.2 -z 0" />
	<arg name="robot_name"  value="Robot1" />
	</include>
        <node pkg="tf" type="static_transform_publisher" name="robot1_broadcaster" args="0 0 0 0 0 0 1 /world /robot1/odom 100" />

</group>


<group ns="robot2">
	<param name="tf_prefix" value="robot2" />
	<include file="$(find agv_model)/launch/one_robot.launch" >
	<arg name="init_pose" value="-x 1.5 -y -4.2 -z 0" />
	<arg name="robot_name"  value="Robot2" />
	</include>
         <node pkg="tf" type="static_transform_publisher" name="robot2_broadcaster" args="0 0 0 0 0 0 1 /world /robot2/odom 100" />
</group>


<group ns="robot3">
	<param name="tf_prefix" value="robot3" />
	<include file="$(find agv_model)/launch/one_robot.launch" >
	<arg name="init_pose" value="-x 0 -y 4.2 -z 0" />
	<arg name="robot_name"  value="Robot3" />
	</include>
         <node pkg="tf" type="static_transform_publisher" name="robot3_broadcaster" args="0 0 0 0 0 0 1 /world /robot3/odom 100" />
</group>


<group ns="robot4">
	<param name="tf_prefix" value="robot4" />
	<include file="$(find agv_model)/launch/one_robot.launch" >
	<arg name="init_pose" value="-x -1.5 -y 4.2 -z 0" />
	<arg name="robot_name"  value="Robot4" />
	</include>
        <node pkg="tf" type="static_transform_publisher" name="robot4_broadcaster" args="0 0 0 0 0 0 1 /world /robot4/odom 100" />
</group>
<!--
-->
</launch>
